{# ======================
List Preloader Animation
===========================
1.Double Bounce loader
2.Audio Wave loader
3.Cube Move Loader
4.Circle Bounce Loader
5.Circle Bounce2 Loader
6.Circle Grid Loader
7.Folding Cube loader
8.Circle loader
=========================#}

{% set preloader_output = '<div class="so-pre-loader">' %}

{% if preloader == 'double-loop' %}
{# ===== 1.Double Bounce loader ======#}
    {% set preloader_output = preloader_output~'<div class="so-loader-center"><div class="spinner-bounce"><div class="double-bounce1"></div><div class="double-bounce2"></div></div></div>'%}
	
{% elseif preloader == 'audio-wave' %}
{# ===== 2.Audio Wave loader======#}
    {% set preloader_output = preloader_output~'<div class="so-loader-center">'%}  
    {% set preloader_output = preloader_output~'<div class="spinner-wave">'%} 
		{% set preloader_output = preloader_output~'<div class="rect1"></div>'%}  
		{% set preloader_output = preloader_output~'<div class="rect2"></div>'%}  
		{% set preloader_output = preloader_output~'<div class="rect3"></div>'%}  
		{% set preloader_output = preloader_output~'<div class="rect4"></div>'%}  
		{% set preloader_output = preloader_output~'<div class="rect5"></div>'%}  
	{% set preloader_output = preloader_output~'</div>'%}  
    {% set preloader_output = preloader_output~'</div>'%}  
	
{% elseif preloader == 'cube-move' %}
{# ===== 3.Cube Move Loader======#}
	{% set preloader_output = preloader_output~'<div class="so-loader-center">'%}  
	{% set preloader_output = preloader_output~'<div class="spinner-cube">'%} 
		{% set preloader_output = preloader_output~'<div class="cube1"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="cube2"></div>'%} 
	{% set preloader_output = preloader_output~'</div>'%}  
	{% set preloader_output = preloader_output~'</div>'%}
	
{% elseif preloader == 'cube-bounce' %}
{# ===== 4.Circle Bounce Loader======#}
	{% set preloader_output = preloader_output~'<div class="so-loader-center">'%}  
	{% set preloader_output = preloader_output~'<div class="spinner-bounce2">'%} 
		{% set preloader_output = preloader_output~'<div class="bounce1"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="bounce2"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="bounce3"></div>'%} 
	{% set preloader_output = preloader_output~'</div>'%}  
	{% set preloader_output = preloader_output~'</div>'%}

{% elseif preloader == 'cube-bounce2' %}
{# ===== 5.Circle Bounce2 Loader======#}
	{% set preloader_output = preloader_output~'<div class="so-loader-center">'%}  
	{% set preloader_output = preloader_output~'<div class="spinner-circle">'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle1 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle2 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle3 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle4 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle5 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle6 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle7 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle8 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle9 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle10 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle11 sk-child"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-circle12 sk-child"></div>'%} 
	{% set preloader_output = preloader_output~'</div>'%}  
	{% set preloader_output = preloader_output~'</div>'%}
	
{% elseif preloader == 'cube-grid' %}
{# ===== 6.Circle Grid Loader======#}
	{% set preloader_output = preloader_output~'<div class="so-loader-center">'%}  
	{% set preloader_output = preloader_output~'<div class="spinner-cube-grid">'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube1"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube2"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube3"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube4"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube5"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube6"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube7"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube8"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube sk-cube9"></div>'%} 
	{% set preloader_output = preloader_output~'</div>'%}  
	{% set preloader_output = preloader_output~'</div>'%}
{% elseif preloader == 'folding-cube' %}
{# ===== 7.Folding Cube loader======#}
	{% set preloader_output = preloader_output~'<div class="so-loader-center">'%}  
	{% set preloader_output = preloader_output~'<div class="spinner-folding-cube">'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube1 sk-cube"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube2 sk-cube"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube4 sk-cube"></div>'%} 
		{% set preloader_output = preloader_output~'<div class="sk-cube3 sk-cube"></div>'%} 
	{% set preloader_output = preloader_output~'</div>'%}  
	{% set preloader_output = preloader_output~'</div>'%}
{% else %}
{# ===== 8.Circle loader ======#}
	{% set preloader_output = preloader_output~'<div class="so-loader-center">'%}  
	{% set preloader_output = preloader_output~'<div class="spinner"></div>'%} 
			{% set preloader_output = preloader_output~'</div>'%}
{% endif %}
{% set preloader_output = preloader_output ~ '</div>' %}

{{preloader_output}}